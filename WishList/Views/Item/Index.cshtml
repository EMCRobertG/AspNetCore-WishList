@model WishList.Models.Item
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>Wishlist</h1>
    <a asp-action="create">Add item</a>
    <ul>
        @foreach (var item in Model.Description) {
            <li>@item <a asp-action="delete" asp-route-id="@Model.Id">delete</a></li>
        }
    </ul>
</body>
</html>
